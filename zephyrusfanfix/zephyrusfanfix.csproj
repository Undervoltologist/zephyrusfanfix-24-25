<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <StartupObject>ZephyrusFanFix.Program</StartupObject>
    <Platforms>AnyCPU;x64</Platforms>
    <ApplicationManifest>app.manifest</ApplicationManifest>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\LpcIO.bin" />
    <None Remove="Resources\PawnIO.sys" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\LpcIO.bin" />
    <EmbeddedResource Include="Resources\PawnIO.sys" />
  </ItemGroup>

</Project>